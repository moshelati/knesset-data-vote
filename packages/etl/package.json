{
  "name": "@knesset-vote/etl",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsx --watch src/cli.ts sync",
    "sync": "tsx src/cli.ts sync",
    "sync:demo": "tsx src/cli.ts sync --demo",
    "aggregate": "tsx src/aggregate/cli-aggregate.ts aggregate",
    "backfill": "tsx src/cli.ts backfill",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "test": "vitest run",
    "type-check": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@knesset-vote/db": "workspace:*",
    "@knesset-vote/shared": "workspace:*",
    "p-limit": "^5.0.0",
    "p-retry": "^6.2.0",
    "pino": "^9.1.0",
    "pino-pretty": "^11.0.0",
    "xml2js": "^0.6.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "tsx": "^4.11.0",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  }
}
