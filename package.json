{
  "name": "knesset-vote",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "test": "turbo run test",
    "test:e2e": "turbo run test:e2e",
    "type-check": "turbo run type-check",
    "db:migrate": "pnpm --filter @knesset-vote/db migrate:dev",
    "db:migrate:deploy": "pnpm --filter @knesset-vote/db migrate:deploy",
    "db:seed": "pnpm --filter @knesset-vote/db seed",
    "db:studio": "pnpm --filter @knesset-vote/db studio",
    "etl:sync": "pnpm --filter @knesset-vote/etl sync",
    "etl:sync:demo": "pnpm --filter @knesset-vote/etl sync:demo",
    "etl:aggregate": "pnpm --filter @knesset-vote/etl aggregate",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\" --ignore-path .gitignore",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\" --ignore-path .gitignore"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "prettier": "^3.2.5",
    "turbo": "^2.0.0",
    "typescript": "^5.4.5"
  },
  "pnpm": {
    "overrides": {
      "typescript": "^5.4.5"
    }
  }
}
